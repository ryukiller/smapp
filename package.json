{
  "name": "spacemesh_app",
  "productName": "Spacemesh",
  "version": "0.2.9",
  "description": "Spacemesh App",
  "keywords": [
    "Apps",
    "Seed",
    "app",
    "cosmic",
    "spacemeshos",
    "smapp"
  ],
  "homepage": "https://github.com/spacemeshos/smapp/blob/develop/README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spacemeshos/smapp.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spacemeshos/smapp/issues"
  },
  "author": {
    "name": "Spacemesh",
    "email": "info@spacemesh.io",
    "url": "https://spacemesh.io/"
  },
  "contributors": [
    {
      "name": "Kirill Shumilov",
      "email": "brusher.designer@gmail.com",
      "url": "https://github.com/brusherru"
    },
    {
      "name": "Maksym Parfenov",
      "email": "maparr@proton.me",
      "url": "https://github.com/maparr"
    }
  ],
  "main": "./src/main/main.ts",
  "scripts": {
    "build": "yarn verify:node-version && concurrently \"yarn build:main\" \"yarn build:renderer\"",
    "build:main": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.main.prod.ts",
    "build:renderer": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.renderer.prod.ts",
    "postinstall": "ts-node .erb/scripts/check-native-dep.js && electron-builder install-app-deps && cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.renderer.dev.dll.ts",
    "build:proto": "proto-loader-gen-types --defaults --oneofs --grpcLib=@grpc/grpc-js --outDir=proto/ proto/*.proto",
    "lint": "cross-env NODE_ENV=development eslint . --cache --ext .js,.jsx,.ts,.tsx --format=pretty",
    "package": "ts-node ./.erb/scripts/clean.js dist && npm run build && electron-builder build --publish never",
    "prepare": "husky install",
    "rebuild": "electron-rebuild --parallel --types prod,dev,optional --module-dir release/app",
    "start": "cross-env NODE_ENV=development ts-node ./.erb/scripts/check-port-in-use.js && npm run verify:node-version && npm run start:renderer",
    "start:main": "cross-env NODE_ENV=development electronmon -r ts-node/register/transpile-only .",
    "start:preload": "cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.preload.dev.ts",
    "start:renderer": "cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack serve --config ./.erb/configs/webpack.config.renderer.dev.ts",
    "test": "jest",
    "clean": "rimraf ./dll ./node_modules",
    "package-mac": "yarn build && node ./scripts/packagerScript.js --target mac",
    "package-win": "yarn build && node ./scripts/packagerScript.js --target windows",
    "package-linux": "yarn build && node ./scripts/packagerScript.js --target linux",
    "verify:node-version": "ts-node ./.erb/scripts/verifyGoSmVersion.ts",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "debug:tx": "ts-node ./scripts/composeTx.ts"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "cross-env NODE_ENV=development eslint --cache"
    ],
    "*.json,.{eslintrc,prettierrc}": [
      "prettier --ignore-path .eslintignore --parser json --write"
    ],
    "*.{css,scss}": [
      "prettier --ignore-path .eslintignore --single-quote --write"
    ],
    "*.{html,md,yml}": [
      "prettier --ignore-path .eslintignore --single-quote --write"
    ]
  },
  "browserslist": [],
  "prettier": {
    "singleQuote": true,
    "overrides": [
      {
        "files": [
          ".prettierrc",
          ".eslintrc"
        ],
        "options": {
          "parser": "json"
        }
      }
    ]
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "release/app/node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "json"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/.erb/mocks/fileMock.js",
      "\\.(css|less|sass|scss)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "./.erb/scripts/check-build-exists.ts"
    ],
    "testEnvironment": "jsdom",
    "testEnvironmentOptions": {
      "url": "http://localhost/"
    },
    "testPathIgnorePatterns": [
      "release/app/dist"
    ],
    "transform": {
      "\\.(ts|tsx|js|jsx)$": "ts-jest"
    }
  },
  "engines": {
    "npm": ">=8.10.0",
    "node": ">=16.15.0"
  },
  "engineStrict": true,
  "build": {
    "productName": "ElectronReact",
    "appId": "org.erb.ElectronReact",
    "asar": true,
    "asarUnpack": "**\\*.{node,dll}",
    "files": [
      "dist",
      "node_modules",
      "package.json"
    ],
    "afterSign": ".erb/scripts/notarize.js",
    "mac": {
      "target": {
        "target": "default",
        "arch": [
          "arm64",
          "x64"
        ]
      },
      "type": "distribution",
      "hardenedRuntime": true,
      "entitlements": "assets/entitlements.mac.plist",
      "entitlementsInherit": "assets/entitlements.mac.plist",
      "gatekeeperAssess": false
    },
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "Development"
    },
    "directories": {
      "app": "release/app",
      "buildResources": "assets",
      "output": "release/build"
    },
    "extraResources": [
      "./assets/**"
    ],
    "publish": {
      "provider": "github",
      "owner": "electron-react-boilerplate",
      "repo": "electron-react-boilerplate"
    }
  },
  "devDependencies": {
    "@jest-runner/electron": "^3.0.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@sentry/utils": "^7.24.1",
